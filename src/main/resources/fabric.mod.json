{
	"schemaVersion": 1,
	"id": "${modId}",
	"version": "${version}",
	"name": "${modName}",
	"description": "${modDescription}",
	"authors": [
		"${authors}"
	],
	"contributors": [
		"${contributors}"
	],
	"contact": {
		"homepage": "${homepage}",
		"issues": "${issues}",
		"sources": "${sources}"
	},
	"license": "${license}",
	"icon": "assets/${modId}/icon.png",
	"mixins": [
		"${modId}.mixins.json",
		{
			"config": "${modId}.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=${fl}",
		"minecraft": ">=${mc}",
		"fabric-api": ">=${fapi}"
	},
	"suggests": {
		"dancerizer": ">=${dancerizer}"
	},
	"entrypoints": {
		"main": [
			"symbolics.division.spirit_vector.SpiritVectorMod"
		],
		"client": [
			"symbolics.division.spirit_vector.SpiritVectorClient"
		],
		"fabric-datagen": [
			"symbolics.division.spirit_vector.SpiritVectorDataGenerator"
		]
	}
}